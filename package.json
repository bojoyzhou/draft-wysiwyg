    "draft-js-entity-props-plugin": "^2.0.0-beta.2",
    "draft-js-focus-plugin": "^2.0.0-beta.2",
    "draft-js-image-plugin": "^2.0.0-beta.2",
    "draft-js-plugins-editor-wysiwyg": "^2.0.0-beta.2",
    "draft-js-resizeable-plugin": "^2.0.0-beta.2",
    "draft-js-table-plugin": "^2.0.0-beta.2",
    "draft-js-toolbar-plugin": "^2.0.0-beta.2",
    "immutable": "^3.7.4"
  },
  "devDependencies": {
    "babel-core": "^6.14.0",
    "babel-loader": "^6.2.5",
