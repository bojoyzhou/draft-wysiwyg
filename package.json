{
  "name": "draft-wysiwyg",
  "version": "0.2.4",
  "author": "Benjamin Kniffler",
  "license": "MIT",
  "repository": "https://github.com/bkniffler/draft-wysiwyg",
  "main": "./lib",
  "engines": {
    "node": "5.x.x",
    "npm": "3.x.x"
  },
  "keywords": [
    "draftjs",
    "draft-js",
    "editor",
    "wysiwyg",
    "drag",
    "drop",
    "react",
    "richtext"
  ],
  "scripts": {
    "start": "node ./example",
    "build": "babel --presets es2015,stage-0,react src/ --out-dir lib/",
