    "express": "^4.13.1",
    "gulp": "^3.9.0",
    "multer": "^1.1.0",
    "react": "^15.0.2",
    "react-dom": "^15.0.2",
    "superagent": "^1.8.0",
    "wrappack": "^0.3.10"
